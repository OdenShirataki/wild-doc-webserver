<wd:session name="bbs" initialize="true" clear_on_close="true">
    <wd:update commit="1">
        <collection name="bbs">
            <field name="name"><wd:print wd:value="wd.input.post.name" /></field>
            <field name="text"><wd:print wd:value="wd.input.post.text" /></field>
            <field name="image_type"><wd:print wd:value="wd.input.post.file.content_type" /></field>
            <field name="image_name"><wd:print wd:value="wd.input.post.file.file_name" /></field>
            <field name="image_data"><wd:print wd:value="wd.input.post.file.data" /></field>
        </collection>
    </wd:update>
</wd:session><wd:script>
    wd.result_options.headers['location']="/bbs-demo/";
</wd:script>