<wd:search
    name="admin"
    collection="admin"
></wd:search><wd:result var="admin" search="admin"><wd:case
    wd:value="wd.v('admin').length==0"
><wd:when wd:value="true"><wd:session name="creatre_account_1st">
    <wd:update commit="1">
        <collection name="admin">
            <field name="name"><wd:print wd:value="wd.input.post.name" /></field>
            <field name="password"><wd:print wd:value="wd.input.post.password" /></field>
        </collection>
    </wd:update>
</wd:session><wd:script>
    wd.result_options.headers['location']="/-db/?registered=1";
</wd:script></wd:when></wd:case></wd:result>